yabai -m config layout bsp
yabai -m config window_placement first_child
yabai -m config window_topmost on
yabai -m config mouse_modifier cmd
yabai -m config mouse_follows_focus on

yabai -m signal --add event=window_created action='node ~/code/yabai-master-stack-plugin/dist/handlers/window-created.js'

yabai -m signal --add event=application_launched action='node ~/code/yabai-master-stack-plugin/dist/handlers/window-created.js'

yabai -m signal --add event=window_moved action='node ~/code/yabai-master-stack-plugin/dist/handlers/window-moved.js'

node ~/code/yabai-master-stack-plugin/dist/handlers/on-yabai-start.js
